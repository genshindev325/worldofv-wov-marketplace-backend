{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "declaration": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "module": "commonjs",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "outDir": "./dist",
    "paths": {
      "@app/arweave": ["libs/arweave/src"],
      "@app/arweave/*": ["libs/arweave/src/*"],
      "@app/blockchain-sync": ["libs/blockchain-sync/src"],
      "@app/blockchain-sync/*": ["libs/blockchain-sync/src/*"],
      "@app/dataloader": ["libs/dataloader/src"],
      "@app/dataloader/*": ["libs/dataloader/src/*"],
      "@app/file-reader": ["libs/file-reader/src"],
      "@app/file-reader/*": ["libs/file-reader/src/*"],
      "@app/file-upload": ["libs/file-upload/src"],
      "@app/file-upload/*": ["libs/file-upload/src/*"],
      "@app/grpc-options": ["libs/grpc-options/src"],
      "@app/grpc-options/*": ["libs/grpc-options/src/*"],
      "@app/login": ["libs/login/src"],
      "@app/login/*": ["libs/login/src/*"],
      "@app/price-conversion-cache": ["libs/price-conversion-cache/src"],
      "@app/price-conversion-cache/*": ["libs/price-conversion-cache/src/*"],
      "@app/redis-client": ["libs/redis-client/src"],
      "@app/redis-client/*": ["libs/redis-client/src/*"],
      "@app/s3": ["libs/s3/src"],
      "@app/s3/*": ["libs/s3/src/*"],
      "@app/sendgrid": ["libs/sendgrid/src"],
      "@app/sendgrid/*": ["libs/sendgrid/src/*"],
      "@app/tracing": ["libs/tracing/src"],
      "@app/tracing/*": ["libs/tracing/src/*"],
      "@app/web3": ["libs/web3/src"],
      "@app/web3/*": ["libs/web3/src/*"],
      "@blockchain/contract": ["libs/contract/src"],
      "@blockchain/contract/*": ["libs/contract/src/*"],
      "@generated/*": ["generated/*"]
    },
    "removeComments": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strictBindCallApply": false,
    "strictNullChecks": false,
    "target": "es2017"
  },
  "files": ["types/express.d.ts", "types/ffprobe.d.ts", "types/thorify.d.ts"],
  "include": ["apps/**/*", "libs/**/*", "common/**/*"]
}
